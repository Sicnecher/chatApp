# Ignore files and directories generated by IDEs and text editors
.idea/
.vscode/
*.sublime-workspace
*.sublime-project
*.suo
*.ntvs*

# Ignore Node.js dependencies
node_modules/

# Ignore compiled output or build directories
dist/
build/
out/

# Ignore log files and debugging information
*.log
logs/
*.log.*
npm-debug.log*
yarn-debug.log*
yarn-error.log*
lerna-debug.log*

# Ignore environment-specific configuration files
.env
.env.local
.env.*.local

# Ignore generated files
*.min.js
*.min.css
*.min.html

# Ignore package-lock files for npm and Yarn
package-lock.json
yarn.lock

# Ignore sensitive or private information (e.g., credentials)
config.js
secrets.json

# Ignore operating system and editor-specific files
.DS_Store
Thumbs.db

# Ignore backup and temporary files
*.bak
*.swp
*.tmp

# Ignore autogenerated documentation
doc/
docs/

# Ignore compiled binary files
*.o
*.a
*.out
*.exe

# Ignore image and media files that can be regenerated
*.png
*.jpg
*.jpeg
*.gif
*.bmp
*.svg
*.ico
*.mp4
*.avi

# Ignore cache and temporary files
.cache/
.tmp/